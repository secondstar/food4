<% content_for :title_section do -%>
	<ul class="cols col1">
		<% if !@head_photo.blank? %>
			<li class="last">
				<div id="head-photo" style="width:310px;">
					
				</div>
				<%= image_tag "http://farm#{@head_photo['farm']}.static.flickr.com/#{@head_photo['server']}/#{@head_photo['flickr_id']}_#{@head_photo['secret']}_m.jpg", :width => '301' %><br />
